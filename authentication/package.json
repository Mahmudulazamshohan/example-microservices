{
  "name": "authentication",
  "version": "0.0.1",
  "description": "Authentication service",
  "author": "mahmudulazamshohan",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:prod": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "webpack:serve": "webpack serve --mode development --live-reload --hot",
    "webpack:prod": "webpack --mode production",
    "build": "npm run build:prod && npm run webpack:prod",
    "seed": "ts-node --project tsconfig.json ./src/seeds/runner.ts",
    "seed:prod": "node --max_old_space_size=512 dist/seeds/runner",
    "start:debug": "npm run wait-for-db && nest start --debug --watch",
    "start:prod": "node --max_old_space_size=512 dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "start:dev": "concurrently \"npm run webpack:serve\" \"npm run start:debug\"",
    "typeorm": "node ./node_modules/typeorm/cli.js -d ./dist/config/data-source.js",
    "create-db": "node dist/scripts/create-db.js",
    "wait-for-db": "node dist/scripts/wait-for-mysql.js",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/Migration",
    "migration:run": "npm run wait-for-db && npm run typeorm migration:run",
    "migration:revert": "npm run wait-for-db && npm run typeorm migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-postman": "ts-node --project tsconfig.json ./src/generate.postman.ts",
    "show": "ts-node --showConfig"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.1",
    "@types/bcryptjs": "^2.4.6",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.7.6",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "mysql2": "^3.6.5",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "postman-collection": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^3.5.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.6",
    "@mui/joy": "^5.0.0-beta.48",
    "@mui/material": "^5.16.6",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@tanstack/react-query": "^5.51.21",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "axios": "^1.7.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "swagger-client": "^3.29.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
