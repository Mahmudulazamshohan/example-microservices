{
  "name": "feed",
  "version": "0.0.1",
  "description": "Feed service",
  "author": "mahmudulazamshohan",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:prod": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "webpack:serve": "webpack serve --mode development --live-reload --hot",
    "webpack:prod": "webpack --mode production",
    "start:debug": "npm run wait-for-db && nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:dev": "concurrently \"npm run start:debug\" \"npm run webpack:serve\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "node ./node_modules/typeorm/cli.js -d ./dist/config/data-source.js",
    "create-db": "node dist/scripts/create-db.js",
    "wait-for-db": "node dist/scripts/wait-for-mysql.js",
    "migration:create": "ts-node ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "npm run typeorm migration:generate ./src/migrations/Migration",
    "migration:run": "npm run wait-for-db && npm run typeorm migration:run",
    "migration:revert": "npm run wait-for-db && npm run typeorm migration:revert",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.ts --context src/components --watch"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.20",
    "@mui/joy": "^5.0.0-beta.48",
    "@mui/material": "^5.15.20",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.3.9",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.1",
    "@opentelemetry/exporter-jaeger": "^1.30.1",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.2.0",
    "mysql2": "^3.6.5",
    "nestjs-pino": "^3.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@opentelemetry/auto-instrumentations-node": "^0.55.3",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.1",
    "@opentelemetry/sdk-node": "^0.57.1",
    "@tanstack/react-query": "^5.51.21",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.7.9",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.2.0",
    "prettier": "^3.0.0",
    "react-router-dom": "^6.26.0",
    "source-map-support": "^0.5.21",
    "style-loader": "^4.0.0",
    "supertest": "^6.3.3",
    "swagger-client": "^3.34.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
